{
  "name": "@topsort/analytics.js",
  "version": "2.0.0",
  "description": "JS library to automatically report events to Topsort's Analytics",
  "main": "dist/ts.js",
  "type": "module",
  "keywords": [
    "ads",
    "sponsored listings",
    "auctions",
    "analytics",
    "topsort"
  ],
  "exports": {
    ".": {
      "import": "./dist/ts.jsm",
      "require": "./dist/ts.js"
    }
  },
  "module": "./dist/ts.js",
  "files": [
    "dist/ts.js",
    "dist/ts.jsm",
    "dist/*.d.ts",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "homepage": "https://github.com/Topsort/analytics.js#readme",
  "scripts": {
    "prepublishOnly": "npm run build && node update-version.js",
    "build": "vite build && tsc --emitDeclarationOnly -p tsconfig.types.json",
    "test": "jest",
    "test:e2e": "vite build && vite build -c vite.config.browser-test.js && tsc && node dist/mocks/api-server.js",
    "lint": "eslint . --max-warnings 0 --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "lint:ci": "npm run lint -- --format=compact",
    "types:check": "tsc"
  },
  "author": "Topsort",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.5.1",
    "nock": "^13.2.9",
    "prettier": "^2.8.7",
    "react": "^18.1.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.4.2",
    "typescript": "^4.8.4",
    "vite": "^4.4.7",
    "vite-plugin-html": "^3.2.0"
  }
}
