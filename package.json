{
  "name": "@topsort/analytics.js",
  "version": "2.6.0",
  "description": "JS library to automatically report events to Topsort's Analytics",
  "main": "dist/ts.js",
  "type": "module",
  "repository": {
    "url": "https://github.com/Topsort/analytics.js"
  },
  "packageManager": "pnpm@10.22.0",
  "keywords": [
    "ads",
    "sponsored listings",
    "auctions",
    "analytics",
    "topsort"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/ts.mjs",
      "require": "./dist/ts.js"
    }
  },
  "module": "./dist/ts.mjs",
  "files": [
    "dist/src",
    "dist/ts.js",
    "dist/ts.mjs",
    "dist/src/*.d.ts",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "homepage": "https://github.com/Topsort/analytics.js#readme",
  "scripts": {
    "build": "vite build",
    "format": "biome format",
    "format:fix": "biome format --write",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "lint:ci": "biome ci",
    "test": "vitest run --coverage",
    "test:e2e": "vite build && vite build -c vite.config.browser-test.js && tsc && node dist/mocks/api-server.js",
    "types:check": "tsc --noemit"
  },
  "author": "Topsort",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "@types/express": "5.0.5",
    "@types/node": "24.10.1",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "@vitest/coverage-v8": "4.0.8",
    "express": "5.2.0",
    "jsdom": "27.2.0",
    "msw": "2.12.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router-dom": "7.9.5",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "vite": "7.2.2",
    "vite-plugin-dts": "4.5.4",
    "vitest": "4.0.8"
  },
  "dependencies": {
    "@topsort/sdk": "0.3.8"
  }
}
